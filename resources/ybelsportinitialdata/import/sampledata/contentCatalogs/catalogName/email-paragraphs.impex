                        <font color='#FFFFFF' size='2' face='Arial, Helvetica, sans-serif' style='padding-left:6px'>
                            <b style='text-transform:uppercase;'>${ctx.messages.details}</b>
                        </font>
                    </td>
                    <td width='40%' height='30' align='left' bgcolor='#000000'>
                        <font color='#FFFFFF' size='2' face='Arial, Helvetica, sans-serif' >
                            <b style='text-transform:uppercase;'>${ctx.messages.description}</b>
                        </font>
                    </td>
                    <td width='10%' height='30' align='left' bgcolor='#000000'>
                        <font color='#FFFFFF' size='2' face='Arial, Helvetica, sans-serif' >
                            <b style='text-transform:uppercase;'>${ctx.messages.ship}</b>
                        </font>
                    </td>
                    <td width='10%' height='30' align='left' bgcolor='#000000'>
                        <font color='#FFFFFF' size='2' face='Arial, Helvetica, sans-serif' style='padding-left:5px'>
                            <b style='text-transform:uppercase;'>${ctx.messages.quantity}</b>
                        </font>
                    </td>
                    <td width='10%' height='30' align='left' bgcolor='#000000'>
                        <font color='#FFFFFF' size='2' face='Arial, Helvetica, sans-serif' style='padding-left:5px'>
                            <b style='text-transform:uppercase;'>${ctx.messages.itemPrice}
                        </font>
                    </td>
                    <td width='10%' height='30' align='left' bgcolor='#000000'>
                        <font color='#FFFFFF' size='2' face='Arial, Helvetica, sans-serif' style='padding-left:5px;padding-right:10px;'>
                           <b style='text-transform:uppercase;'>${ctx.messages.total}</b>
                        </font>
                    </td>
                </tr>
                #foreach( $entry in ${ctx.consignment.entries} )
                <tr>
                    <td valign='top' align='left' style='padding-left:10px;padding-right:10px'>
                        <a href='${ctx.baseUrl}/$entry.orderEntry.product.url'>
                            #foreach($image in $entry.orderEntry.product.images)
                                #if($image.imageType == 'PRIMARY' && $image.format == 'thumbnail' )
                                    <img style='height:100px;max-height:100px;max-width:100px;width:100px;'
                                         src='${ctx.mediaBaseUrl}$image.url' width='100' height='100'
                                         alt='$entry.orderEntry.product.name' title='$entry.orderEntry.product.name' />
                                #end
                            #end
                        </a>
                    </td>
                    <td valign='top' align='left' style='padding-top:10px;padding-right:5px;'>
                        <a href='${ctx.secureBaseUrl}$entry.orderEntry.product.url'>
                            <font color='#000' size='2' face='Arial, Helvetica, sans-serif'><b>$entry.orderEntry.product.name</b></font>
                        </a> <br/>
                        <font color='#000' size='2' face='Arial, Helvetica, sans-serif'>$entry.orderEntry.product.code</font> <br/>
                         #if (!$entry.orderEntry.product.baseOptions.isEmpty())
                            #foreach ($option in $entry.orderEntry.product.baseOptions)
                                #if ($option.selected && ($option.selected.url == $entry.orderEntry.product.url))
                                    <table width='100%' cellpadding='0' cellspacing='0'>
                                        #foreach ($selectedOption in $option.selected.variantOptionQualifiers)
                                        <tr>
                                            <td width='30%'>
                                                <font color='#000' size='2' face='Arial, Helvetica, sans-serif'>$selectedOption.name:</font>
                                            </td>
                                            <td width='70%'>
                                                <font color='#000' size='2' face='Arial, Helvetica, sans-serif'>$selectedOption.value</font>
                                            </td>
                                        </tr>
                                        #end
                                    </table>
                                #end
                            #end
                        #end
                        #if (!$ctx.order.appliedProductPromotions.isEmpty())
                            #foreach( $promotion in $ctx.order.appliedProductPromotions)
                                #set ($displayed = false)
                                #foreach ($consumedEntry in $promotion.consumedEntries)
                                    #if (!$displayed && ($consumedEntry.orderEntryNumber == $entry.orderEntry.entryNumber))
                                        #set ($displayed = true)
                                        <font color='#414a4f' size='2' face='Arial, Helvetica, sans-serif'><b>${promotion.description}</b></font>
                                    #end
                                #end
                             #end
                        #end
                    </td>
                    <td valign='top' align='left' style='padding-top:10px;'>
                        <font color='#000000' size='2' face='Arial, Helvetica, sans-serif'>${ctx.messages.shipMethod}</font>
                    </td>
                    <td valign='top' align='center' style='padding-top:10px;'>
                        <font color='#000000' size='2' face='Arial, Helvetica, sans-serif'>$entry.orderEntry.quantity</font>
                    </td>
                    <td valign='top' align='center' style='padding-top:10px;'>
                        <font color='#000000' size='2' face='Arial, Helvetica, sans-serif'>$entry.orderEntry.basePrice.formattedValue</font>
                    </td>
                    <td valign='top' align='center' style='padding-top:10px;'>
                        <font color='#000000' size='2' face='Arial, Helvetica, sans-serif'>
                            <b>
                                #if($entry.orderEntry.totalPrice.value > 0) $entry.orderEntry.totalPrice.formattedValue
                                #else ${ctx.messages.free}
                                #end
                            </b>
                        </font>
                    </td>
                </tr>
                #end
            </table>
            </td></tr></table>
            <!--products end-->
            <br/>
            <p><font color='#000' size='2' face='Arial, Helvetica, sans-serif'>${ctx.messages.getMessage('doNotReply')}</font></p>
            <p><font color='#000' size='2' face='Arial, Helvetica, sans-serif'>${ctx.messages.getMessage('orderDetails')}</font></p>
            <p><font color='#000' size='2' face='Arial, Helvetica, sans-serif'>${ctx.messages.getMessage('support')}</font></p>
        </td>
    </tr>
    <tr>
        <td align='left' style='border-top: #000000 solid 1px; background-color:#0072CE;'>
            <font color='#ffffff' size='3' face='Arial , Helvetica, sans-serif'>${ctx.messages.benefits}</font>
        </td>
    </tr>
    <tr>
        <td align='left' style='padding-bottom:10px; background-color:#0072CE;text-align: center;' class='products'>
            <a style='color:#FFFFFF' href='${ctx.messages.websiteLink}' target='_blank'>
                <b><font size='4' face='Arial , Helvetica, sans-serif' style='text-transform: uppercase;'>${ctx.messages.website}</font></b>
            </a>
        </td>
    </tr>
</table>
