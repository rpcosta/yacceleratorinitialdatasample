INSERT_UPDATE CMSParagraphComponent;$contentCV[unique=true];uid[unique=true];content[lang=$lang]
                                   ;;CustomerRegistrationEmailBody;"
#macro(genHtmlLinkStartTag $url)
<a href='$url'><font color='#666666'>
#end
#macro(genHtmlLinkEndTag)
</font></a>
#end
#macro(genHtmlLink $url $bodyContent)
<a href='$url'><font color='#666666'>$bodyContent</font></a>
#end
#set ($secureAccountUrl = '${ctx.secureBaseUrl}/my-account')
#set ($securePswdResetUrl = '${ctx.secureBaseUrl}/login/pw/request/external')
#set ($mailToUrl = 'mailto:${ctx.messages.contactUsEmailAddress}')
#set ( $instructionsContactUs = ${ctx.messages.getMessage('instructionsContactUs', &#34;&#35;genHtmlLinkStartTag(${ctx.messages.contactUsPage})&#34;, &#34;&#35;genHtmlLinkEndTag()&#34;, &#34;&#35;genHtmlLink($mailToUrl ${ctx.messages.contactUsEmailAddress})&#34;)} )
    <p><font color='#666666' size='2' face='Arial, Helvetica, sans-serif'><b>${ctx.messages.getMessage('salutation', ${ctx.title},${ctx.displayName})}</b>,</font></p>
    <p><font color='#666666' size='2' face='Arial, Helvetica, sans-serif'>${ctx.messages.thankYouForRegistering}</font></p>
    <p><font color='#333' size='2' face='Arial, Helvetica, sans-serif'>${ctx.messages.registeredEmailInfo}: ${ctx.email}.</font></p>
    <p><font color='#666666' size='2' face='Arial, Helvetica, sans-serif'>${ctx.messages.instructionsShoppingUrl}: <a href='${ctx.baseUrl}'><font color='#666666'>${ctx.baseUrl}</font></a> <br>
    ${ctx.messages.getMessage('instructionsAccountUrl', &#34;&#35;genHtmlLinkStartTag($secureAccountUrl)&#34;, &#34;&#35;genHtmlLinkEndTag()&#34;)}<br>
    ${ctx.messages.getMessage('instructionsForgottenPswdUrl', &#34;&#35;genHtmlLinkStartTag($securePswdResetUrl)&#34;, &#34;&#35;genHtmlLinkEndTag()&#34;)}</font></p>
    <p><font color='#666666' size='2' face='Arial, Helvetica, sans-serif'>$instructionsContactUs</font></p>
    <p><font color='#666666' size='2' face='Arial, Helvetica, sans-serif'>${ctx.messages.complimentaryClosing}</font></p>
    <p><font color='#666666' size='2' face='Arial, Helvetica, sans-serif'>${ctx.messages.signature}</font></p>"

INSERT_UPDATE CMSParagraphComponent;$contentCV[unique=true];uid[unique=true];content[lang=$lang]
                                   ;;DeliverySentEmailBody;"
#macro( genHtmlFont $text )
<font color='#414a4f' size='2' face='Arial, Helvetica, sans-serif'><b>$text</b></font>
#end
#macro(genHtmlLinkStartTag $url)
<a href='$url'><font color='#000'>
#end
#macro(genHtmlLinkEndTag)
</font></a>
#end
#macro(genHtmlLink $url $bodyContent)
<a href='$url'><font color='#000'>$bodyContent</font></a>
#end
<table>
    <tr>
        <td>&nbsp;</td>
    </tr>
    <tr>
        <td align='left' valign='top'>
            <p>
            #if (!$ctx.isGuest())
                <font color='#000' size='2' face='Arial, Helvetica, sans-serif'>${ctx.messages.getMessage('salutation', ${ctx.customerName})}</font>
            #end
            #if ($ctx.isGuest())
                <font color='#000' size='2' face='Arial, Helvetica, sans-serif'>${ctx.messages.getMessage('salutation', '')}</font>
            #end
            </p>            <p><font color='#000' size='2' face='Arial, Helvetica, sans-serif'>${ctx.messages.getMessage('sendInformation', ${ctx.consignment.code}, ${ctx.consignment.trackingID})}</font></p>
            <p><font color='#000' size='2' face='Arial, Helvetica, sans-serif'>${ctx.messages.getMessage('sendDetails')}</font></p>
            <p><font color='#000' size='2' face='Arial, Helvetica, sans-serif'>${ctx.messages.getMessage('sendRecomendations')}</font></p>
        </td>
    </tr>
</table>
<table width='60%' border='0' align='left' cellpadding='0' cellspacing='0' bordercolor='#51585c' class='products' style='border: 1px solid #51585c;margin-left:30px !important;'>
	<tr>
		<td>
			<table>
				<tr>
					<td width='33%' valign='top' rowspan='2'>
						<font color='#333' size='2' face='Arial, Helvetica, sans-serif'><b>${ctx.messages.deliveryAddress}</b></font>
							<p style='margin:3px 0'>
								<font color='#666666' size='2' face='Arial, Helvetica, sans-serif'>
									${ctx.address.firstName} ${ctx.address.lastName}<br/>
									${ctx.address.streetName}<br/>
                                    ${ctx.address.streetNumber}<br/>
                                    ${ctx.address.houseDeptmt}<br/>
                                    ${ctx.address.communeCL.name}<br/>
                                    ${ctx.address.regionCL.name}<br/>
									${ctx.address.country.name}
								</font>
							</p>
					</td>
				</tr>
			</table>
		</td>
	</tr>
</table>
<table align='center' cellpadding='0' cellspacing='0'>
	<tr>
		<td>
            <br/>
            <!--products start-->
            <table width='100%' border='1' align='center' cellpadding='0' cellspacing='0' bordercolor='#bfc1c0' class='products'>
                <tr>
                    <td>
                        <table width='100%' cellpadding='0' cellspacing='0'>
                                        <tr style='background-color: #000; color: #FFF;'>
                                            <td width='110px' style='width:110px;padding-left:10px;padding-right:10px' height='40px'><font color='#FFF' size='2' face='Arial, Helvetica, sans-serif' style='text-transform: uppercase'><p style='margin:3px 0'><b>${ctx.messages.details}</p></td>
                                            <td height='40px'><font color='#FFF' size='2' face='Arial, Helvetica, sans-serif' style='text-transform: uppercase'><b>${ctx.messages.description}</td>
                                            <td width='12%' height='40px'><font color='#FFF' size='2' face='Arial, Helvetica, sans-serif' style='text-transform: uppercase'><b>${ctx.messages.ship}</b></font></td>
                                            <td width='12%' height='40px'><font color='#FFF' size='2' face='Arial, Helvetica, sans-serif' style='text-transform: uppercase'><b>${ctx.messages.quantity}</b></font></td>
                                            <td width='12%' height='40px'><font color='#FFF' size='2' face='Arial, Helvetica, sans-serif' style='text-transform: uppercase'><b>${ctx.messages.itemPrice}</b></font></td>
                                            <td width='12%' height='40px'><font color='#FFF' size='2' face='Arial, Helvetica, sans-serif' style='text-transform: uppercase'><b>${ctx.messages.total}</b></font></td>
                            </tr>
                            #foreach( $entry in ${ctx.consignment.entries} )
                            <tr>
                                            <td colspan='2'>
                                    <table width='100%' border='0' cellpadding='0' cellspacing='0'>
                                        <tr>
                                            <td valign='top' width='110px' style='width:110px;padding-left:10px;padding-right:10px'>
                                                            <a href='${ctx.baseUrl}/$entry.orderEntry.product.url'>
                                                    #foreach($image in $entry.orderEntry.product.images) #if($image.imageType == 'PRIMARY' && $image.format == 'thumbnail' )
                                                                <img src='${ctx.mediaBaseUrl}$image.url' alt='' title='$entry.orderEntry.product.name' />
                                                    #end #end
                                                </a>
                                            </td>
                                            <td valign='top'>
                                                            <p style='margin:3px 0'><font color='#000' size='2' face='Arial, Helvetica, sans-serif'>
                                                                <a href='${ctx.secureBaseUrl}$entry.orderEntry.product.url'><font color='#000'><b>$entry.orderEntry.product.name</b></font></a>
                                                                <br/>
                                                                $entry.orderEntry.product.code
                                                </p>
                                                #if (!$entry.orderEntry.product.baseOptions.isEmpty())
                                                #foreach ($option in $entry.orderEntry.product.baseOptions)
                                                #if ($option.selected && ($option.selected.url == $entry.orderEntry.product.url))
                                                <table width='100%' cellpadding='0' cellspacing='0'>
                                                    #foreach ($selectedOption in $option.selected.variantOptionQualifiers)
                                                    <tr>
                                                                    <td width='30%'><p style='margin:3px 0'><font color='#000' size='2' face='Arial, Helvetica, sans-serif'>$selectedOption.name:</font></p></td>
                                                                    <td width='70%'><p style='margin:3px 0'><font color='#000' size='2' face='Arial, Helvetica, sans-serif'>$selectedOption.value</font></p></td>
                                                    </tr>
                                                    #end
                                                </table>
                                                #end
                                                #end
                                                #end
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                                    <td valign='top'><p><font color='#000' size='2' face='Arial, Helvetica, sans-serif'>
                                                ${ctx.messages.shipMethod}
                                                </font></p></td>
                                    <td valign='top'><p style='margin:3px 0 3px 20px'><font color='#000' size='2' face='Arial, Helvetica, sans-serif'>$entry.orderEntry.quantity</font></p></td>
                                    <td valign='top'><p style='margin:3px 0'><font color='#000' size='2' face='Arial, Helvetica, sans-serif'>$entry.orderEntry.basePrice.formattedValue</font></p></td>
                                <td valign='top'><p style='margin:3px 0'><font color='#414a4f' size='3' face='Arial, Helvetica, sans-serif'><b>#if($entry.orderEntry.totalPrice.value > 0) $entry.orderEntry.totalPrice.formattedValue #else ${ctx.messages.free} #end</b></font></p></td>
                            </tr>
                            #end
                        </table>
                    </td>
                </tr>
            </table>
            <!--products end-->
            <br/>
            <p><font color='#000' size='2' face='Arial, Helvetica, sans-serif'>${ctx.messages.getMessage('doNotReply')}</font></p>
            <p><font color='#000' size='2' face='Arial, Helvetica, sans-serif'>${ctx.messages.getMessage('orderDetails')}</font></p>
            <p><font color='#000' size='2' face='Arial, Helvetica, sans-serif'>${ctx.messages.getMessage('support')}</font></p>
            <table width='100%' border='0' cellspacing='0' cellpadding='0' style='background-color:#ffd600' class='products'>
                <tr>
                    <td style='padding: 5px; text-align: center; text-transform: uppercase;'>
                        <b><font color='#000' size='4' face='Arial, Helvetica, sans-serif'><a href='${ctx.messages.websiteLink}'>${ctx.messages.website}</a></font></b>
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>"

INSERT_UPDATE CMSParagraphComponent;$contentCV[unique=true];uid[unique=true];content[lang=$lang]
                                   ;;ForgottenPasswordEmailBody;"
#macro(genHtmlLinkStartTag $url)
<a href='$url'><font color='#666666'>
#end
#macro(genHtmlLinkEndTag)
</font></a>
#end
#macro(genHtmlLink $url $bodyContent)
<a href='$url'><font color='#666666'>$bodyContent</font></a>
#end
