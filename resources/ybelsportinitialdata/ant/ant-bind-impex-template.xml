		<attribute name="addonname"/>

		<sequential>
			<for list="coredata,sampledata" param="data">
				<sequential>
					<for list="contentCatalogs/catalogName,productCatalogs/catalogName,stores/storeName" param="catalog">
						<sequential>
							<var name="destPath" value="${ext.@{addonname}.path}/resources/@{addonname}/import/@{data}/@{catalog}/"/>
							<if>
								<available file="${destPath}"/>
								<then>
									<for list="${rootPackageToken},${extensionUidToken},${extensionNameToken},${customerGroupIdToken},${customerGroupNameEnToken},${customerGroupNameEsClToken},${defaultEmployeePasswordToken},${themeUidToken},${themeNameEnToken},${themeNameEsClToken},${vendorUidToken},${vendorNameToken},${brandNameToken},${contentCatalogUidToken},${contentCatalogNameEnToken},${contentCatalogNameEsClToken},${productCatalogUidToken},${productCatalogNameEnToken},${productCatalogNameEsClToken},${classificationCatalogUidToken},${classificationCatalogNameEnToken},${classificationCatalogNameEsClToken},${syncJobProductCatalogToken},${storeNameToken},${storeUidToken},${storeNameEnToken},${storeNameEsClToken},${storeSalesOrgCodeToken},${storePromotionGroupUidToken},${siteNameEnToken},${siteNameEsClToken},${siteDomainLowercaseToken},${siteAliasToken},${siteAliasLowercaseToken},${fromEmailToken},${fromNameToken},${storeEmailTemplatePrefixToken},${storeSpecificLandingPageUidToken},${storeSpecificLandingLayoutPageUidToken},${storeSpecificLandingTemplateToken},${categoryPromotionsUidToken},${categoryPromotionsNameEnToken},${categoryPromotionsNameEsClToken},${categoryCampainsUidToken},${categoryCampainsNameEnToken},${categoryCampainsNameEsClToken},${solrSearchConfigUidToken},${solrSearchConfigNameToken},${solrSearchPrefixNameToken},${solrIndexedTypeToken},${solrPriceRangeUidToken},${solrSearchProfileUidToken},${solrSearchProfileNameEnToken},${solrSearchProfileNameEsClToken},${warehouseDeliverySampleUidToken},${warehousePickupSampleUidToken},${warehouseSampleEmailToken},${warehouseDeliverySampleNameToken},${warehousePickupSampleNameToken},${googleLocationsJobCodeToken},${googleProductsJobCodeToken},${googleExportJobCodeToken},${googleHostToken},${googleUsernameToken},${googlePasswordToken},${samplePromotion1UidToken},${samplePromotion1NameEnToken},${samplePromotion1NameEsClToken},${samplePromotion1SourceRuleUidToken},${samplePromotion1JsonDescriptionEnToken},${samplePromotion1JsonNameEnToken},${samplePromotion1JsonAppliedMessageEnToken},${samplePromotion1JsonDescriptionEsClToken},${samplePromotion1JsonNameEsClToken},${samplePromotion1JsonAppliedMessageEsClToken},${samplePromotion1CategoryCodeToken},${samplePromotion2UidToken},${samplePromotion2NameEnToken},${samplePromotion2NameEsClToken},${samplePromotion2SourceRuleUidToken},${samplePromotion2JsonDescriptionEnToken},${samplePromotion2JsonNameEnToken},${samplePromotion2JsonAppliedMessageEnToken},${samplePromotion2JsonDescriptionEsClToken},${samplePromotion2JsonNameEsClToken},${samplePromotion2JsonAppliedMessageEsClToken},${sampleCategoryNameEnToken},${sampleCategoryNameEsClToken},${sampleCategoryPrefixToken},${sampleCategoryGuideSizePrefixToken},${sampleProductMaterialNameToken},${sampleProductBrandNameToken},${cmsStoreToken},${cmsImageToken},${cmsSocialFacebookToken},${cmsSocialInstagramToken},${cmsSocialTwitterToken},${cmsContactPhoneToken},${cmsContactPhoneSFToken},${cmsEmailCVToken},${cmsEmailCambiosToken}" param="token">
										<sequential>
											<replace dir="${destPath}" token="${token}" value="@{token}">
												<include name="**/*.impex"/>
											</replace>
											<echo message="@{addonname}: token [${token}] was replaced by [@{token}]"/>
										</sequential>
									</for>
								</then>
								<else>
									<echo message="No impex templates found for @{addonname}, location: ${destPath}"/>
								</else>
							</if>
						</sequential>
					</for>
				</sequential>
			</for>
		</sequential>
	</macrodef>

</project>
